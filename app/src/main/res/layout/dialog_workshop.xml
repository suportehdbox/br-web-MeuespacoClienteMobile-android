<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:background="#D9000000">

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginBottom="@dimen/margin_default_4"
        android:src="@drawable/status_40"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_default_4"
        android:gravity="center"
        android:textSize="@dimen/font_default_5"
        android:textColor="@color/text_default_15"
        android:text="@string/title_dialog_workshop"/>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_default_1"
        android:gravity="left"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_default_6"
            android:layout_marginRight="@dimen/margin_default"
            android:layout_marginBottom="@dimen/margin_default_1"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_3"
            android:textColor="@color/text_default_1"
            android:textStyle="bold"
            android:text="."/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_default_2"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_0"
            android:textColor="@color/text_default_1"
            android:text="@string/message_dialog_workshop_1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_default_1"
        android:gravity="left"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_default_6"
            android:layout_marginRight="@dimen/margin_default"
            android:layout_marginBottom="@dimen/margin_default_1"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_3"
            android:textColor="@color/text_default_1"
            android:textStyle="bold"
            android:text="."/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_default_2"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_0"
            android:textColor="@color/text_default_1"
            android:text="@string/message_dialog_workshop_2"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_default_1"
        android:gravity="left"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_default_6"
            android:layout_marginRight="@dimen/margin_default"
            android:layout_marginBottom="@dimen/margin_default_1"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_3"
            android:textColor="@color/text_default_1"
            android:textStyle="bold"
            android:text="."/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_default_2"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_0"
            android:textColor="@color/text_default_1"
            android:text="@string/message_dialog_workshop_3"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_default_1"
        android:gravity="left"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_default_1"
        android:gravity="left"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_default_6"
            android:layout_marginRight="@dimen/margin_default"
            android:layout_marginBottom="@dimen/margin_default_1"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_3"
            android:textColor="@color/text_default_1"
            android:textStyle="bold"
            android:text="."/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_default_2"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_0"
            android:textColor="@color/text_default_1"
            android:text="@string/message_dialog_workshop_4"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_default_1"
        android:gravity="left"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_default_6"
            android:layout_marginRight="@dimen/margin_default"
            android:layout_marginBottom="@dimen/margin_default_1"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_3"
            android:textColor="@color/text_default_1"
            android:textStyle="bold"
            android:text="."/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_default_2"
            android:layout_gravity="center"
            android:textSize="@dimen/font_default_0"
            android:textColor="@color/text_default_1"
            android:text="@string/message_dialog_workshop_5"/>

    </LinearLayout>


    <ImageView
        android:id="@+id/iv_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_default_6"
        android:layout_marginTop="@dimen/margin_default_4"
        android:layout_marginRight="@dimen/margin_default_6"
        android:filterTouchesWhenObscured="true"
        android:src="@drawable/bt_ok_text_white" />


</LinearLayout>
