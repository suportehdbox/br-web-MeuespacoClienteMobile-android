<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/table_layout" 
    android:layout_marginTop="25sp">

    <TableRow style="@style/input_field_row" >

        <EditText
            android:id="@+id/first_name"
            style="@style/edit_text"
            android:hint="@string/first_name_label"
            android:inputType="textCapWords|textFilter"
            android:nextFocusDown="@+id/last_name" />
    </TableRow>

    <TableRow style="@style/input_field_row" >

        <EditText
            android:id="@+id/last_name"
            style="@style/edit_text"
            android:hint="@string/last_name_label"
            android:inputType="textCapWords|textFilter"
            android:nextFocusDown="@+id/home_phone" />
    </TableRow>

    <TableLayout
        android:id="@+id/home_phone_section"
        style="@style/wrap_height_wrap_width" >

            <EditText
                android:id="@+id/home_phone"
                style="@style/edit_text"
                android:hint="@string/home_phone_label"
                android:inputType="phone"
                android:maxLength="17"
                android:nextFocusDown="@+id/mobile_phone" />
    </TableLayout>

    <TableLayout
        android:id="@+id/mobile_phone_section"
        style="@style/wrap_height_wrap_width" >

            <EditText
                android:id="@+id/mobile_phone"
                style="@style/edit_text"
                android:hint="@string/mobile_phone_label"
                android:inputType="phone"
                android:maxLength="17"
                android:nextFocusDown="@+id/email" />
    </TableLayout>

    <TableLayout
        android:id="@+id/email_section"
        style="@style/wrap_height_wrap_width" >

            <EditText
                android:id="@+id/email"
                style="@style/edit_text"
                android:hint="@string/email_address_label"
                android:inputType="textEmailAddress"
                android:maxLength="50"
                android:nextFocusDown="@+id/city" />
    </TableLayout>

</TableLayout><!-- From: file:/TFS/LS-MobileApps/Main/LibertySegurosBrasil-Android/app/src/main/res/layout/contact.xml -->