    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/option_heading_bg"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/imageheading_Iv"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical"
            android:paddingLeft="10dp" />

        <TextView
            android:id="@+id/titleheading_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:shadowColor="@color/ColorTextHeaderShadow"
            android:text=""
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/ColorTextHeader"
            android:textStyle="bold" />
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="14dp"
        android:background="#DADADA" />

    <LinearLayout
        android:id="@+id/newcase_layout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="#ffffff"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/titleheading_tv_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:paddingLeft="15dp"
            android:shadowColor="@color/ColorTextHeaderShadow"
            android:text="@string/TitleScreenNewAssistance"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/ColorMenu"
            android:textStyle="bold" />
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="14dp"
        android:background="#DADADA" />

    <LinearLayout
        android:id="@+id/callforAssistance_layout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/bottom_round_rect"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:shadowColor="@color/ColorTextHeaderShadow"
            android:text="@string/CallForAssistance24h"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/ColorMenu"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_gravity="right|center_vertical"
            android:paddingLeft="20dp"
            android:src="@drawable/call_assistencia_icon_ligar" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/mycases_layout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/mycases_bg"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:shadowColor="@color/ColorTextHeaderShadow"
            android:text="@string/MyFiles"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/ColorMenu"
            android:textStyle="bold" />
    </LinearLayout>
   
